{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Yarp": "Information"
    }
  },
  "RateLimiting": {
    "Global": {
      "PermitLimit": 60,
      "QueueLimit": 30,
      "WindowMinutes": 1
    },
    "Concurrency": {
      "PermitLimit": 30,
      "QueueLimit": 15
    }
  },
  "ReverseProxy": {
    "Clusters": {
      "auth-cluster": {
        "Destinations": {
          "auth-1": {
            "Address": "http://auth-api-1:80"
          },
          "auth-2": {
            "Address": "http://auth-api-2:80"
          },
          "auth-3": {
            "Address": "http://auth-api-3:80"
          }
        }
      }
    }
  }
}
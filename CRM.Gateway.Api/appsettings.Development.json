{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Yarp": "Debug"
    }
  },
  "RateLimiting": {
    "Global": {
      "PermitLimit": 1000,
      "QueueLimit": 100
    }
  },
  "ReverseProxy": {
    "Clusters": {
      "auth-cluster": {
        "LoadBalancingPolicy": "FirstAlphabetical",
        "SessionAffinity": {
          "Enabled": false
        },
        "HealthCheck": {
          "Active": {
            "Enabled": false
          },
          "Passive": {
            "Enabled": false
          }
        },
        "Destinations": {
          "auth-1": {
            "Address": "http://localhost:5049"
          }
        }
      }
    }
  }
}